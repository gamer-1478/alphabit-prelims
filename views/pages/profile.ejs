    <button onclick="Logout()">
        Logout
    </button>
</div>

<script>
    async function Logout() {
        await fetch(
            '/logout', {
                method: "delete",
                redirect: 'follow'
            }).then(response => {
            if (response.redirected) {
                window.location.href = response.url;
            }
        })
    }
</script>